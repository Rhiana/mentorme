<div class = "container-fluid">
<span id="notice"><%= notice %></span>
<div class = "row">
  <div class = "col-xs-12 navigation">
    <%= link_to 'Home', root_path %>
    <%= link_to 'Edit', edit_profile_path(@profile) %>
    <%= link_to 'Back', profiles_path %>
  </div>
</div>

<div class = "row">
<div class = "col-xs-3 search" id="profile_search">
<h1>Search for profiles</h1>



</div>



<div class = "col-xs-9 profile">
<p>
  <%= gravatar_for @profile %>
</p>

<p>
  <strong>Name:</strong>
  <%= @profile.name %>
</p>

<p>
  <strong>Mentor:</strong>
  <%= @profile.mentor %>
</p>

<p>
  <strong>Mentee:</strong>
  <%= @profile.mentee %>
</p>

<p>
  <strong>Post code:</strong>
  <%= @profile.post_code %>
</p>

<p>
  <strong>Email:</strong>
  <%= @profile.email %>
</p>

<p>
  <strong>Tag line:</strong>
  <%= @profile.tag_line %>
</p>

<p>
  <strong>Meet up:</strong>
  <%= @profile.meet_up %>
</p>

<p>
  <strong>Catagory:</strong>
  <%= @profile.catagory %>
</p>

<p>
  <strong>Short biography:</strong>
  <%= @profile.short_biography %>
</p>

<p>
  <strong>What do you want to achieve:</strong>
  <%= @profile.what_do_you_want_to_achieve %>
</p>
</div>
</div>
</div>

